channel=1;
dbfile='G:\PicoFiles\test\experiment.db';
exportfolder='G:\PicoFiles\test\export';
if ~isfolder(exportfolder); mkdir(exportfolder);end
info=crxReader(dbfile);

numwells=numel(info.Wells);
for w=1:numwells
    crxReader(dbfile,'well',info.Wells(w),'channel',channel,'tile','all','saveas',[exportfolder '\' info.name],'info',info,'verbose',1);
end
